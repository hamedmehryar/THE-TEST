{
  "name": "express-app1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set NODE_PATH=./dist && node dist/App.js",
    "dev": "set NODE_ENV=dev && nodemon -r tsconfig-paths/register src/App.ts",
    "build": "tsc -p .",
    "test": "set NODE_ENV=test && mocha --watch --check-leaks -r ts-node/register -r tsconfig-paths/register \"tests/**/*.spec.ts\""
  },
  "description": "ExpressApp1",
  "author": {
    "name": "Hamed Mehryar"
  },
  "main": "app.js",
  "dependencies": {
    "debug": "^2.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.14.0",
    "mongoose": "^6.0.12"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.37",
    "@types/express-serve-static-core": "^4.0.50",
    "@types/mime": "^1.3.1",
    "@types/mocha": "^9.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^14.14.7",
    "@types/serve-static": "^1.7.32",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.0.5"
  }
}
